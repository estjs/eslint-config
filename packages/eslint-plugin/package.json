	"license": "MIT",
	"funding": "https://github.com/sponsors/estjs",
	"homepage": "https://github.com/estjs/@estjs/eslint-configc#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/estjs/@estjs/eslint-config.git"
	},
	"bugs": "https://github.com/estjs/@estjs/eslint-config/issues",
	"keywords": [
		"eslint-plugin",
		"eslint"
	],
	"sideEffects": false,
