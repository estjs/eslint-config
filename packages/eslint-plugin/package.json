				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"lint": "eslint ./src"
	},
	"dependencies": {
		"@typescript-eslint/utils": "^5.57.1",
		"@unocss/config": "^0.50.6",
		"@unocss/core": "^0.50.6",
		"magic-string": "^0.30.0",
		"synckit": "^0.8.5"
	},
	"devDependencies": {
		"eslint": "^8.37.0",
		"unbuild": "^1.2.0"
	}
