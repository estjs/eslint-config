{
  "name": "@estjs/eslint-config",
  "version": "1.1.3",
  "packageManager": "pnpm@9.4.0",
  "description": "",
  "type": "module",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/estjs/eslint-config#readme",
  "bugs": "https://github.com/estjs/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/eslint-config.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "pnpm run build &&eslint --cache --fix ./",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks --access public"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0"
  },
  "dependencies": {
    "@eslint-types/jsdoc": "48.2.1",
    "@eslint-types/typescript-eslint": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@unocss/eslint-plugin": "^0.61.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.12",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-svelte": "^2.40.0",
    "eslint-plugin-toml": "^0.11.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-yml": "^1.14.0",
    "globals": "^15.6.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.7",
    "bumpp": "^9.4.1",
    "eslint": "^9.0.0",
    "pnpm": "^9.4.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
