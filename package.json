{
  "name": "@estjs/eslint-config",
  "version": "2.0.5",
  "packageManager": "pnpm@10.11.0",
  "description": "",
  "type": "module",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/estjs/eslint-config#readme",
  "bugs": "https://github.com/estjs/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/eslint-config.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "cross-env NAPI_RS_FORCE_WASI=1 pnpm run build && eslint --cache --fix ./",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks --access public",
    "test": "pnpm run build && vitest",
    "test:watch": "pnpm run build && vitest watch"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "prettier": ">=3"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/markdown": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@unocss/eslint-plugin": "^66.1.2",
    "@vitest/eslint-plugin": "^1.2.1",
    "deepmerge-ts": "^7.1.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.13.3",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-oxlint": "^1.3.0",
    "eslint-plugin-oxlint-x": "workspace:*",
    "eslint-plugin-pnpm": "^0.3.1",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.1.0",
    "eslint-plugin-yml": "^1.18.0",
    "globals": "^16.2.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "vue-eslint-parser": "^10.1.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/node": "^22.15.26",
    "@unrs/resolver-binding-wasm32-wasi": "^1.7.2",
    "@unrs/resolver-binding-win32-x64-msvc": "1.7.2",
    "bumpp": "^10.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "oxlint": "^0.5.1",
    "pnpm": "^10.11.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": ">=5.8.3",
    "unocss": "^66.1.2",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18"
  }
}
