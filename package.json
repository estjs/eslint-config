{
  "name": "@estjs/eslint-config",
  "version": "2.1.2",
  "packageManager": "pnpm@10.14.0",
  "description": "",
  "type": "module",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/estjs/eslint-config#readme",
  "bugs": "https://github.com/estjs/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/eslint-config.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": ["biome.json", "dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/*", "./dist/index.d.ts"]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && cp biome.json dist",
    "lint": "pnpm run build && eslint --cache --fix ./",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks --access public",
    "test": "pnpm run build && vitest",
    "test:watch": "pnpm run build && vitest watch"
  },
  "peerDependencies": {
    "@biomejs/biome": ">=2.1.3",
    "eslint": ">=9",
    "prettier": ">=3"
  },
  "dependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/markdown": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@unocss/eslint-plugin": "^66.3.3",
    "@vitest/eslint-plugin": "^1.3.4",
    "deepmerge-ts": "^7.1.5",
    "eslint-config-biome": "^1.9.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-biome": "^0.9.19",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^52.0.2",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-pnpm": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-yml": "^1.18.0",
    "globals": "^16.3.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "vue-eslint-parser": "^10.2.0",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/node": "^22.17.0",
    "bumpp": "^10.2.2",
    "eslint": "^9.32.0",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "pnpm": "^10.14.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": ">=5.8.3",
    "unocss": "^66.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  }
}
