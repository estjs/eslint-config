{
  "name": "@estjs/eslint-config",
  "version": "2.0.0-beta.16",
  "packageManager": "pnpm@10.7.0",
  "description": "",
  "type": "module",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/estjs/eslint-config#readme",
  "bugs": "https://github.com/estjs/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/eslint-config.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "biome.json",
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && cp biome.json dist",
    "lint": "pnpm run build && eslint --cache --fix ./ ",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks --access public",
    "test": "pnpm run build && vitest",
    "test:watch": "pnpm run build && vitest watch"
  },
  "peerDependencies": {
    "@biomejs/biome": ">=1.9.4",
    "eslint": ">=9",
    "prettier": ">=3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@unocss/eslint-plugin": "^66.0.0",
    "@vitest/eslint-plugin": "^1.1.38",
    "deepmerge-ts": "^7.1.5",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-biome": "^0.9.19",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.9.4",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "vue-eslint-parser": "^10.1.1",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.13.14",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "pnpm": "^10.7.0",
    "tsup": "^8.4.0",
    "typescript": ">=5.8.2",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=18"
  }
}
