{
  "name": "@estjs/eslint-config",
  "version": "2.1.6",
  "packageManager": "pnpm@10.29.2",
  "description": "",
  "type": "module",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/estjs/eslint-config#readme",
  "bugs": "https://github.com/estjs/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/eslint-config.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "pnpm run build && eslint --cache --fix ./",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks --access public",
    "test": "pnpm run build && vitest",
    "test:watch": "pnpm run build && vitest watch"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "prettier": ">=3"
  },
  "dependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/markdown": "^7.5.1",
    "@prettier/plugin-oxc": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@unocss/eslint-plugin": "^66.6.0",
    "@vitest/eslint-plugin": "^1.6.6",
    "deepmerge-ts": "^7.1.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^62.5.4",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-pnpm": "^1.5.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-regexp": "^3.0.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.7.0",
    "eslint-plugin-yml": "^3.1.2",
    "globals": "^17.3.0",
    "jsonc-eslint-parser": "^2.4.2",
    "local-pkg": "^1.1.2",
    "vue-eslint-parser": "^10.2.0",
    "yaml-eslint-parser": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.12",
    "bumpp": "^10.4.1",
    "eslint": "^9.35.0",
    "execa": "^9.6.1",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.3",
    "pnpm": "^10.29.2",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": ">=5.9.3",
    "unocss": "^66.6.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
